{
  "name": "codesandbox-sandpack-vue3",
  "version": "1.0.1",
  "description": "Vue3 components that give you the power of editable sandboxes that run in the browser.",
  "keywords": [
    "vue3",
    "sandpack",
    "sandpack-vue3",
    "@codesandbox",
    "@codesandbox/sandpack-vue",
    "@codesandbox/sandpack-vue3",
    "codesandbox"
  ],
  "homepage": "https://github.com/jerrywu001/sandpack-vue3#readme",
  "bugs": {
    "url": "https://github.com/jerrywu001/sandpack-vue3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerrywu001/sandpack-vue3.git"
  },
  "license": "MIT",
  "author": "jerrywu001 <57242263@163.com>",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "global.d.ts"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "build": "tsup",
    "build:vite": "vite build",
    "dev": "tsup --watch",
    "preinstall": "npx only-allow npm",
    "lint": "eslint **/*.{ts,tsx,vue} --ext .ts,.tsx,.vue",
    "play": "npm run storybook --workspace playground",
    "play:dev": "npm run dev --workspace playground",
    "play:dev-pack": "npm run dev:usepack --workspace playground",
    "prepare": "chmod +x .husky/* && husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push && npm publish",
    "stylelint": "stylelint **/*.{css,less,scss,vue}",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@codemirror/closebrackets": "~0.19.2",
